  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "ci": "npm run test && npm run document",
    "prebuild": "npm run lint",
    "build": "tsc",
    "document": "typedoc --out docs lib --theme minimal",
    "lint": "eslint **/*.ts",
    "pretest": "npm run build",
    "test": "nyc mocha -r ts-node/register test/integration/*.ts"
  },
  "main": "./lib/virtualbox",
  "mocha": {
    "files": {
      "glob": "./test/**/*.ts"
    },
    "options": {
      "compilers": {
        "ts": "ts-node/register"
      }
    },
    "requires": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**/*",
      "test/**/*",
      ".eslintrc.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Node-Virtualization/node-virtualbox.git"
  },
  "dependencies": {
    "log4js": "^4.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.15.3",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "async": "^2.6.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^6.2.2",
    "nodeunit": "^0.11.3",
    "nyc": "^15.0.0",
    "sinon": "^7.5.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "typedoc": "^0.15.4",
    "typescript": "^3.7.3"
  },
  "engines": {
    "engine": "node >= 0.4.1"
  }
}
